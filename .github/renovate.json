{
  "packageRules": [
    {
      "matchManagers": ["github-actions"],
      "groupName": "CI dependencies",
      "groupSlug": "ci-deps",
      "separateMajorMinor": false
    }
  ],
  "dependencyDashboard": false,
  "enabledManagers": ["github-actions"],
  "labels": ["dependencies"],
  "rebaseWhen": "behind-base-branch",
  "rangeStrategy": "pin"
}
